<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.myscheduler.FirstFragment"
        android:label="@string/一覧表示"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
        <action
            android:id="@+id/action_to_scheduleEditFragment"
            app:destination="@id/action_to_scheduleEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.myscheduler.SecondFragment"
        android:label="@string/カレンダー表示"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
        <action
            android:id="@+id/action_to_scheduleEditFragment"
            app:destination="@id/action_to_scheduleEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/action_to_scheduleEditFragment"
        android:name="com.example.myscheduler.ScheduleEditFragment"
        android:label="@string/schedule_edit_text"
        tools:layout="@layout/fragment_schedule_edit" />
</navigation>